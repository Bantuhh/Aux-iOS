  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

  pod 'Spartan'

  pod 'SDWebImage'

  pod 'youtube-ios-player-helper'
    

  target 'AuxTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
